{
    "compilerOptions": {
        "target": "es2022",
        "declaration": true,
        "module": "esnext",
        "strict": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,

        /* https://github.com/cypress-io/cypress/issues/29519 */
        "skipLibCheck": true,

        "types": ["cypress"],
        "lib": ["es2022", "dom"]
    }
}
