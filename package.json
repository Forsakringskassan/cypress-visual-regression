{
  "name": "@forsakringskassan/cypress-visual-regression",
  "version": "3.0.0",
  "description": "Module for adding visual regression testing to Cypress",
  "keywords": [
    "visual regression",
    "image diff",
    "cypress"
  ],
  "homepage": "https://github.com/Forsakringskassan/cypress-visual-regression",
  "bugs": "https://github.com/Forsakringskassan/cypress-visual-regression/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forsakringskassan/cypress-visual-regression.git"
  },
  "license": "MIT",
  "author": "Försäkringskassan",
  "files": [
    "dist",
    "commands.js",
    "commands.d.ts",
    "plugin.js",
    "plugin.d.ts"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:dts": "dts-bundle-generator -o dist/commands.d.ts src/commands.d.ts && dts-bundle-generator -o dist/plugin.d.ts src/plugin.d.ts",
    "build:esbuild": "esbuild --bundle --platform=node --target=node20 --outdir=dist src/commands.js src/plugin.js",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "lint": "npm run eslint",
    "prepack": "release-prepack --bundle",
    "postpack": "release-postpack",
    "prepublishOnly": "release-prepublish --bundle",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "postpublish": "release-postpublish",
    "pretest": "run-s prettier:check eslint",
    "test": "jest"
  },
  "commitlint": {
    "extends": [
      "@forsakringskassan/commitlint-config/no-jira"
    ]
  },
  "lint-staged": {
    "*.{js,json,md,ts}": "prettier --write",
    "*.{js,ts}": "eslint --fix"
  },
  "prettier": "@forsakringskassan/prettier-config",
  "release": {
    "extends": "@forsakringskassan/semantic-release-config"
  },
  "jest": {
    "preset": "@forsakringskassan/jest-config"
  },
  "dependencies": {
    "pngjs": "7.0.0"
  },
  "devDependencies": {
    "@forsakringskassan/commitlint-config": "3.0.9",
    "@forsakringskassan/eslint-config": "13.0.6",
    "@forsakringskassan/eslint-config-jest": "13.0.1",
    "@forsakringskassan/eslint-config-typescript": "13.0.7",
    "@forsakringskassan/jest-config": "30.0.0",
    "@forsakringskassan/prettier-config": "3.1.7",
    "@html-validate/release-scripts": "6.9.6",
    "@types/jest": "30.0.0",
    "@types/node": "20.19.17",
    "cypress": "15.3.0",
    "dts-bundle-generator": "9.5.1",
    "esbuild": "0.25.10",
    "npm-pkg-lint": "4.1.0",
    "npm-run-all2": "8.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "cypress": "^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0"
  },
  "engines": {
    "node": ">= 20"
  }
}
